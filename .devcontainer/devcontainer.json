{
	"name": "nodedit",
	"service": "workspace",
	"workspaceFolder": "/work",
	"dockerComposeFile": "docker-compose.yml",
	"onCreateCommand": "pnpm install",
	"customizations": {
		"vscode": {
			"extensions": [
				"SamKirkland.plop-templates",
				"dbaeumer.vscode-eslint",
				"eamodio.gitlens",
				"sysoev.language-stylus"
			]
		}
	},
	"remoteUser": "node"
}